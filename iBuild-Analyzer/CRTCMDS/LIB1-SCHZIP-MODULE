-include makefile_defs

SCHZIP.MODULE : QRPGLESRC.FILE/SCHZIP.MBR \
    CUSMSTL2.FILE \
    SCHZIPD.FILE
	# DBGVIEW(*STMT)
	# TGTRLS(V7R4M0)
ifdef LIB1-SCHZIP-MODULE-PARMS
	$(eval @_PARMS = $(LIB1-SCHZIP-MODULE-PARMS))
else
	$(eval @_PARMS = $(CRTRPGMODPARMS))
endif
	system -v -k "CRTRPGMOD $(@_PARMS)  SRCFILE(QRPGLESRC) SRCMBR(SCHZIP) TGTCCSID(37) PRFDTA(*NOCOL) SRTSEQ(*HEX) LANGID(*JOBRUN) MODULE($(LIB1)/SCHZIP) STGMDL(*INHERIT)"

CUSMSTL2.FILE : QDDSSRC.FILE/CUSMSTL2.MBR \
    CUSMST.FILE
	system -v -k "EDOCRTOBJ BLDTGT($(LIB1)/CUSMSTL2) OBJTYPE(*FILE) OBJATTR(LF) CMD(CRTLF) CMDPARMS(' FILE($(LIB1)/CUSMSTL2) SRCFILE(QDDSSRC) SRCMBR(CUSMSTL2) TEXT(''Customer Master L2'')')"

CUSMST.FILE : QDDSSRC.FILE/CUSMST.MBR
	system -v -k "EDOCRTOBJ BLDTGT($(LIB1)/CUSMST) OBJTYPE(*FILE) OBJATTR(PF) CMD(CRTPF) CMDPARMS(' FILE($(LIB1)/CUSMST) SRCFILE(QDDSSRC) SRCMBR(CUSMST) TEXT(''Customer Master'')')"

SCHZIPD.FILE : QDDSSRC.FILE/SCHZIPD.MBR \
    CUSMSTL2.FILE
	system -v -k "EDOCRTOBJ BLDTGT($(LIB1)/SCHZIPD) OBJTYPE(*FILE) OBJATTR(DSPF) CMD(CRTDSPF) CMDPARMS(' FILE($(LIB1)/SCHZIPD) SRCFILE(QDDSSRC) SRCMBR(SCHZIPD) DFRWRT(*YES) RSTDSP(*NO) TEXT(''Customer Search by Zip'')')"

