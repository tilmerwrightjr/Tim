-include makefile_defs

MNTMENU.FILE : QDDSSRC.FILE/MNTMENU.MBR \
    CUSMSTL1.FILE
	system -v -k "EDOCRTOBJ BLDTGT($(LIB1)/MNTMENU) OBJTYPE(*FILE) OBJATTR(DSPF) CMD(CRTDSPF) CMDPARMS(' FILE($(LIB1)/MNTMENU) SRCFILE(QDDSSRC) SRCMBR(MNTMENU) DFRWRT(*YES) RSTDSP(*NO) TEXT(''Customer Master Maintenance'')')"

CUSMSTL1.FILE : QDDSSRC.FILE/CUSMSTL1.MBR \
    CUSMST.FILE
	system -v -k "EDOCRTOBJ BLDTGT($(LIB1)/CUSMSTL1) OBJTYPE(*FILE) OBJATTR(LF) CMD(CRTLF) CMDPARMS(' FILE($(LIB1)/CUSMSTL1) SRCFILE(QDDSSRC) SRCMBR(CUSMSTL1) TEXT(''Customer Master L1'')')"

CUSMST.FILE : QDDSSRC.FILE/CUSMST.MBR
	system -v -k "EDOCRTOBJ BLDTGT($(LIB1)/CUSMST) OBJTYPE(*FILE) OBJATTR(PF) CMD(CRTPF) CMDPARMS(' FILE($(LIB1)/CUSMST) SRCFILE(QDDSSRC) SRCMBR(CUSMST) TEXT(''Customer Master'')')"

