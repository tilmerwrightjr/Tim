-include makefile_defs

SCHNAMD.FILE : QDDSSRC.FILE/SCHNAMD.MBR \
    CUSMSTL3.FILE
	system -v -k "EDOCRTOBJ BLDTGT($(LIB1)/SCHNAMD) OBJTYPE(*FILE) OBJATTR(DSPF) CMD(CRTDSPF) CMDPARMS(' FILE($(LIB1)/SCHNAMD) SRCFILE(QDDSSRC) SRCMBR(SCHNAMD) DFRWRT(*YES) RSTDSP(*NO) TEXT(''Customer Search by Name'')')"

CUSMSTL3.FILE : QDDSSRC.FILE/CUSMSTL3.MBR \
    CUSMST.FILE
	system -v -k "EDOCRTOBJ BLDTGT($(LIB1)/CUSMSTL3) OBJTYPE(*FILE) OBJATTR(LF) CMD(CRTLF) CMDPARMS(' FILE($(LIB1)/CUSMSTL3) SRCFILE(QDDSSRC) SRCMBR(CUSMSTL3) TEXT(''Customer Master L3'')')"

CUSMST.FILE : QDDSSRC.FILE/CUSMST.MBR
	system -v -k "EDOCRTOBJ BLDTGT($(LIB1)/CUSMST) OBJTYPE(*FILE) OBJATTR(PF) CMD(CRTPF) CMDPARMS(' FILE($(LIB1)/CUSMST) SRCFILE(QDDSSRC) SRCMBR(CUSMST) TEXT(''Customer Master'')')"

